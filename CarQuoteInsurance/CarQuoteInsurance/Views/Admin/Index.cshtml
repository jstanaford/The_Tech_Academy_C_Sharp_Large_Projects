@using CarQuoteInsurance.ViewModels
@model List<GetQuoteVm>

<div class="jumbotron">
	<h1 class="display-4">Welcome, Admin.</h1>
	<p class="lead">This is a page where we keep records of those inquirying car insurance for our business needs.</p>
</div>


<div class="row no-gutters m-3">
	<div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
		<div class="card-header text-center bg-primary">
			<h5 class="m-0 text-white">Sign-ups</h5>
		</div>
		<div class="card-body">
			@foreach (var carOwner in Model)
			{

				<input type="hidden" name="Id" value="@carOwner.Id" />
				<div class="row mb-3">
					<div class="col-2">
						@carOwner.FirstName
					</div>
					<div class="col">
						@carOwner.LastName
					</div>
					<div class="col-4">
						@carOwner.EmailAddress
					</div>
					<div class="col-4">
						Payment Per Month: @carOwner.PaymentPerMonth
					</div>
				</div>

			}
		</div>
	</div>
</div>